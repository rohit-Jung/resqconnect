{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": ["^node", "^[a-z]", "^@/", "^~", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
